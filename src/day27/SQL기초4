show database;
show variable like 'datadir';
create database mydbtest1;
drop database mydbtest1;
drop database if exists mydbtest1;
use mydbtest1;

drop database if exists test1;
create database test1;
show databases;
show variable like'datadir';
use test1;

테이블 생성과 타입관련

create database mydb1120;
use mydb1120;
create table member(아이디 text, 패스워드 text,이름 text);
show tables;
drop table if exists member;

[테이블예제1]
use mydb1120;
create table board(content long text, writer varchar(30),pwd int);
select*from board;
drop table if exists board;

[테이블예제2]
drop database if exists mydb1120;
create database mydb1120;
show databases;
use mydb1120;
drop table if exists member;
[테이블 생성: 테이블명(속성명 속성타입,속성명 속성타입)]
create table member(
정수필드1 tinyint,정수필드2 smallint,정수필드3 midiumint,정수필드4 int,정수필드5 bigint,
실수필드1 float,실수필드2 double,실수필드3 decimal,
문자열필드1 char(100),문자열필드2 varchar(100),문자열필드3 text,문자열필드4 long text,
논리필드 bool);
show tables;
select*from member;


)


